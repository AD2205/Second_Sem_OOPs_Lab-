public class MyClass {
    public static void main(String args[]) {
      int x=10;
      int y=25;
      int z=100;
      if(x>y && x>z)
      {
          System.out.println("x is greater among all");
      }
      else if(y>z)
      {
          System.out.println("y is greater among all");
      }
      else
      {
           System.out.println("z is greater among all");
      }

    }
}
