public class MyClass {
    public static void main(String args[]) {
     int a=15,b=30,gcd=1,lcm=1;
     
     for(int i=1;i<=a &&i<=b;i++)
     {
         if(a%i==0 && b%i==0)
         {
             gcd=i;
         }
     }
     lcm=(a*b)/gcd;
     
     System.out.println("The lcm of "+a+" and "+b+" is "+lcm);
     
    }
}
