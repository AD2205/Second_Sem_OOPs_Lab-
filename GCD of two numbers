public class MyClass {
    public static void main(String args[]) {
      int a=10,b=14,gcd=1;
      
      for(int i=1;i<=a && i<=b;i++)
      {
          if(a%i==0 && b%i==0)
          {
              gcd=i;
          }
      }
      System.out.println("The gcd of "+a+" and "+b+" is "+gcd);
    }
}
